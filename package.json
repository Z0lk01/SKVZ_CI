{
  "devDependencies": {
    "cypress": "^13.15.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.2.0"
  },
  "dependencies": {
    "cypress-parallel": "^0.12.0",
    "moment": "^2.30.1"
  },
   "scripts": {
  "test": "cypress run",
  "merge:reports": "npx mochawesome-merge e2e/reports/*.json > e2e/reports/merged-report.json",
  "generate:report": "npx marge e2e/reports/merged-report.json --reportDir e2e/reports --reportFilename report.html",
  "report": "npm run test && npm run merge:reports && npm run generate:report"
}

}


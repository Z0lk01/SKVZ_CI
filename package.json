{
  "devDependencies": {
    "cypress": "^13.15.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.2.0"
  },
  "dependencies": {
    "cypress-parallel": "^0.15.0",
    "moment": "^2.30.1"
  },
  "scripts": {
    "test": "cypress run",
    "merge:reports": "npx mochawesome-merge cypress/reports/*.json > cypress/reports/merged-report.json",
    "generate:report": "npx marge cypress/reports/merged-report.json --reportDir cypress/reports --reportFilename report.html",
    "report": "npm run test && npm run merge:reports && npm run generate:report"
  }
}
